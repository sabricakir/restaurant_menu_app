<div id="<%= dom_id food %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Name:</strong>
    <%= food.name %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Price:</strong>
    <%= food.price %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Contents:</strong>
    <%= food.contents %>
  </p>

</div>
